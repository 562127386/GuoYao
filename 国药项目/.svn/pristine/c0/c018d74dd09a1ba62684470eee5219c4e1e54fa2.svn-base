    <meta content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0" name="viewport" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/css/framework-font.css" rel="stylesheet" />
    <link href="~/Content/mobile/BUI/src/css/bui.css?v=1.0.4" rel="stylesheet" />
    <script src="~/Content/js/jquery/jquery-2.1.1.min.js"></script>
    <script src="~/Content/js/validate/jquery.metadata.js"></script>
    <script src="~/Content/js/validate/jquery.validate.min.js"></script>
    <script src="~/Content/mobile/BUI/src/js/plugins/fastclick.js"></script>
    <script src="~/Content/mobile/BUI/src/js/bui.js"></script>
    <link href="~/Content/mobile/dialogM/mobile/need/layer.css?v=1.0.1" rel="stylesheet" />
    <script src="~/Content/mobile/dialogM/mobile/layer.js"></script>
    <link href="~/Content/css/framework-bui.css" rel="stylesheet" />  
    <script src="~/Content/js/common/framework-bui.js?v=1.0.6"></script>   

    <script src="~/Content/mobile/BUI/fly-zomm-img.min.js"></script>
